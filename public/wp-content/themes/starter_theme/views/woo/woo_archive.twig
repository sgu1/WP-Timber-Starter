{% extends 'base.twig' %}

{% block content %}

<p>A light shines in the dusky skies above the flowing green leaves blowing in the wind. It's hope beyond all of the noise.</p>

    {% do action('woocommerce_before_main_content') %}

    <div class="before-shop-loop">
        {% do action('woocommerce_before_shop_loop') %}
    </div>

    <div class="loop">
        {% for post in products %}
            {% include ["woo/woo_tease-product.twig"] %}
        {% endfor %}
    </div>

    {% do action('woocommerce_after_shop_loop') %}
    {% do action('woocommerce_after_main_content') %}

{% endblock content %}